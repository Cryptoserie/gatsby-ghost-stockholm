{
    "plugins": [
        [
            "prismjs",
            {
                "languages": [
                    "markup",
                    "css",
                    "javascript",
                    "http",
                    "json",
                    "markdown",
                    "python",
                    "java",
                    "groovy",
                    "html",
                    "yaml",
                    "toml",
                    "jsx",
                    "graphql",
                    "sql",
                    "jinja2"
                ],
                "plugins": ["line-numbers", "normalize-whitespace"],
                "theme": "default",
                "css": true
            }
        ]
    ],
    "presets": ["babel-preset-gatsby"]
}
